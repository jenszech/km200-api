{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "modulePathIgnorePatterns": ["__mocks__"],
  "collectCoverage": true,
  "automock": false,
  "setupFiles": [
    "./setupJest.js"
  ]
}